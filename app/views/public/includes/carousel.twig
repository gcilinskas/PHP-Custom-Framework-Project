<div id="carousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carousel" data-slide-to="0" class="active"></li>
        <li data-target="#carousel" data-slide-to="1"></li>
        <li data-target="#carousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <div class="lazyload fill fadeFirstImage" style="background-image:url('../../../assets/img/slider-1.jpg');"></div>
            <div class="carousel-caption">
                <h2 class="fadeInRight">Alistore Products</h2>
                <p class="animated fadeInUp">Get Best Prices</p>
                <p class="animated fadeInUp"><a href="/products" class="btn btn-transparent btn-rounded btn-large">Shop Now</a></p>
            </div>
        </div>
        <div class="lazyload carousel-item">
            <div class="fill fadeImage" style="background-image:url('../../../assets/img/slider-2.jpg');"></div>
            <div class="carousel-caption">
                <h2 class="fadeInRight">Alistore Products</h2>
                <p class="animated fadeInUp">Get Best Prices</p>
                <p class="animated fadeInUp"><a href="/products" class="btn btn-transparent btn-rounded btn-large">Shop Now</a></p>
            </div>
        </div>
        <div class="lazyload carousel-item">
            <div class="fill" style="background-image:url('../../../assets/img/slider-3.jpg');"></div>
            <div class="carousel-caption">
                <h2 class="fadeInRight">Alistore Products</h2>
                <p class="animated fadeInUp">Get Best Prices</p>
                <p class="animated fadeInUp"><a href="/products" class="btn btn-transparent btn-rounded btn-large">Shop Now</a></p>
            </div>
        </div>
    </div>
    <a class="carousel-control-prev" href="#carousel" role="buttton" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carousel" role="buttton" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>










